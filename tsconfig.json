{
    "compilerOptions": {
        "outDir": "./dist/",
        "lib": ["dom", "dom.iterable", "esnext"],
        "target": "ES5",
        "module": "ES6",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "downlevelIteration": true
    },
    "include": ["./src/**/*"]
}
